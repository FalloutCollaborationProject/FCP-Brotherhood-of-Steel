<Defs>

    <!--__________ Unique Lost Hills PawnKinds __________-->

    <!--_____ Lost Hills Brotherhood of Steel _____-->

    <!-- Head Paladin Rhombus -->
    
    <PawnKindDef ParentName="FCP_BoSkindBase">
        <defName>FCP_Unique_Pawnkind_Head_Paladin_Rhombus</defName>
        <label>Head Paladin Rhombus</label>
        <defaultFactionDef>FCP_Faction_BoS_Lost_Hills</defaultFactionDef>
        <combatPower>95</combatPower>
        <itemQuality>Excellent</itemQuality>
        <gearHealthRange>1.0~1.0</gearHealthRange>
        <apparelRequired>
            <li MayRequire="Rick.FCP.PowerArmor">FCP_Apparel_Power_Armor_Frame</li>
            <li>FCP_Apparel_BoS_Jumpsuit</li>
            <li MayRequire="Rick.FCP.PowerArmor">FCP_Apparel_BoS_Lost_Hills_T51_Power_Armor</li>
            <li MayRequire="Rick.FCP.PowerArmor">FCP_Apparel_Power_Armor_T51_Helmet</li>
        </apparelRequired>
        <forcedTraits>
            <FCP_Trait_Power_Armor_Trained />
        </forcedTraits>
        <weaponMoney>2000~2800</weaponMoney>
        <weaponTags>
            <li MayRequire="Rick.FCP.EnergyWeapons">FCP_Heavy_Laser_Weapon</li>
        </weaponTags>
        <initialWillRange>6~7</initialWillRange>
        <initialResistanceRange>28~38</initialResistanceRange>
        <backstoryFiltersOverride>
            <li>
                <categoriesChildhood>
                    <li>FCP_Character_Backstory_Rhombus_Child</li>
                </categoriesChildhood>
                <categoriesAdulthood>
                    <li>FCP_Character_Backstory_Rhombus_Adult</li>
                </categoriesAdulthood>
            </li>
        </backstoryFiltersOverride>
        <inventoryOptions>
            <skipChance>0.0</skipChance>
            <subOptionsChooseOne>
                <li>
                    <thingDef>FCP_Currency_BoS_Scrip</thingDef>
                    <countRange>100~180</countRange>
                </li>
                <li>
                    <thingDef>MealSurvivalPack</thingDef>
                    <countRange>3~5</countRange>
                </li>
                <li>
                    <thingDef>MedicineIndustrial</thingDef>
                    <countRange>2~4</countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
    </PawnKindDef>

