    <!--__________ Unique Fallout 3 PawnKinds __________-->

    <!--_____ Lyons' Brotherhood of Steel _____-->

    <!-- Elder Owyn Lyons -->
    
    <PawnKindDef ParentName="FCP_BoSkindBase">
        <defName>FCP_Unique_Pawnkind_Elder_Owyn_Lyons</defName>
        <label>Elder Owyn Lyons</label>
        <defaultFactionDef>FCP_Faction_BoS_Lyons_Pride_Remnants</defaultFactionDef>
        <factionLeader>true</factionLeader>
        <combatPower>25</combatPower>
        <itemQuality>Excellent</itemQuality>
        <gearHealthRange>1.0~1.0</gearHealthRange>
        <apparelRequired>
            <li>FCP_Apparel_BoS_Elder_Robes</li>
        </apparelRequired>
        <apparelDisallowTags>
            <li>FCP_Power_Armor_Helmet</li>
            <li>FCP_Power_Armor</li>
            <li>FCP_Power_Armor_Lyons</li>
            <li>FCP_Power_Armor_Outcast</li>
        </apparelDisallowTags>
        <weaponMoney>900~1500</weaponMoney>
        <weaponTags>
            <li MayRequire="Rick.FCP.EnergyWeapons">FCP_Laser_Weapon</li>
        </weaponTags>
        <initialWillRange>5~6</initialWillRange>
        <initialResistanceRange>18~23</initialResistanceRange>
        <backstoryFiltersOverride>
            <li>
                <categoriesChildhood>
                    <li>FCP_Character_Backstory_Owyn_Lyons_Child</li>
                </categoriesChildhood>
                <categoriesAdulthood>
                    <li>FCP_Character_Backstory_Owyn_Lyons_Adult</li>
                </categoriesAdulthood>
            </li>
        </backstoryFiltersOverride>
        <inventoryOptions>
            <skipChance>0.0</skipChance>
            <subOptionsChooseOne>
                <li>
                    <thingDef>FCP_Currency_BoS_Scrip</thingDef>
                    <countRange>100~200</countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
    </PawnKindDef>

