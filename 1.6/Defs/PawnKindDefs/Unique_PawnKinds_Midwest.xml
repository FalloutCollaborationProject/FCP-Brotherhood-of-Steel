    <!--__________ Unique Midwestern BoS PawnKinds __________-->

    <!--_____ Midwestern Brotherhood of Steel _____-->

    <!-- General Simon Barnaky -->
    
    <PawnKindDef ParentName="FCP_BoSkindBase">
        <defName>FCP_Unique_Pawnkind_General_Barnaky</defName>
        <label>General Barnaky</label>
        <defaultFactionDef>FCP_Faction_BoS_Midwest</defaultFactionDef>
        <combatPower>110</combatPower>
        <itemQuality>Legendary</itemQuality>
        <gearHealthRange>1.0~1.0</gearHealthRange>
        <apparelRequired>
            <li MayRequire="Rick.FCP.PowerArmor">FCP_Apparel_Power_Armor_Frame</li>
            <li>FCP_Apparel_BoS_Jumpsuit</li>
            <li MayRequire="Rick.FCP.PowerArmor">FCP_Apparel_BoS_Midwestern_Power_Armor</li>
            <li MayRequire="Rick.FCP.PowerArmor">FCP_Apparel_BoS_Midwestern_Power_Armor_Helmet</li>
        </apparelRequired>
        <forcedTraits>
            <FCP_Trait_Power_Armor_Trained />
        </forcedTraits>
        <weaponMoney>2500~3200</weaponMoney>
        <weaponTags>
            <li MayRequire="Rick.FCP.EnergyWeapons">FCP_Heavy_Laser_Weapon</li>
        </weaponTags>
        <initialWillRange>7~8</initialWillRange>
        <initialResistanceRange>32~42</initialResistanceRange>
        <backstoryFiltersOverride>
            <li>
                <categoriesChildhood>
                    <li>FCP_Character_Backstory_Barnaky_Child</li>
                </categoriesChildhood>
                <categoriesAdulthood>
                    <li>FCP_Character_Backstory_Barnaky_Adult</li>
                </categoriesAdulthood>
            </li>
        </backstoryFiltersOverride>
        <inventoryOptions>
            <skipChance>0.0</skipChance>
            <subOptionsChooseOne>
                <li>
                    <thingDef>FCP_Currency_BoS_Scrip</thingDef>
                    <countRange>150~250</countRange>
                </li>
                <li>
                    <thingDef>MealSurvivalPack</thingDef>
                    <countRange>4~6</countRange>
                </li>
                <li>
                    <thingDef>MedicineIndustrial</thingDef>
                    <countRange>3~5</countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
    </PawnKindDef>

