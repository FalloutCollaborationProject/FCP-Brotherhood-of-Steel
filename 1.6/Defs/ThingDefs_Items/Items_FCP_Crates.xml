<Defs>

    <!--__________ BoS Crates __________-->

    <!--_____ Crates _____-->

    <!-- Standard Weapon Crate -->

    <ThingDef ParentName="ResourceBase">
        <defName>FCP_Item_BoS_Standard_Weapon_Crate</defName>
        <label>BoS Weapon Crate</label>
        <description>A supply crate containing Brotherhood of Steel energy weapons. The Brotherhood favors laser technology for its reliability and ease of maintenance in the field. Contains standard-issue laser pistols, rifles, and advanced variants used by Brotherhood Knights.</description>
        <graphicData>
            <texPath>Things/Items/BoS_Crates/BoS_Food_and_Uniform_Crate/FCP_BoS_Food_and_Uniform_Crate</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>.75</drawSize>
        </graphicData>
        <size>(1,1)</size>
        <resourceReadoutPriority>Middle</resourceReadoutPriority>
        <soundInteract>Metal_Drop</soundInteract>
        <soundDrop>Metal_Drop</soundDrop>
        <stackLimit>5</stackLimit>
        <useHitPoints>true</useHitPoints>
        <statBases>
            <MarketValue>300</MarketValue>
            <MaxHitPoints>100</MaxHitPoints>
            <Mass>2</Mass>
            <Flammability>0.2</Flammability>
        </statBases>
        <thingCategories>
            <li>ItemsMisc</li>
        </thingCategories>
        <drawGUIOverlay>true</drawGUIOverlay>
        <tradeability>Sellable</tradeability>
        <techLevel>Industrial</techLevel>
        <comps>
            <li Class="CompProperties_Usable">
                <useJob>UseItem</useJob>
                <useLabel>Open {0}</useLabel>
            </li>
            <li Class="FCP.Core.CompProperties_UseEffectItemBox">
                <numWeightedDrops>2</numWeightedDrops>
                <guaranteedDrops>
                </guaranteedDrops>
                <weightedDrops>
                    <li MayRequire="Rick.FCP.EnergyWeapons">
                        <thingDef>FCP_Gun_Laser_Pistol</thingDef>
                        <countRange>1</countRange>
                    </li>
                    <li MayRequire="Rick.FCP.EnergyWeapons">
                        <thingDef>FCP_Gun_Laser_Rifle</thingDef>
                        <countRange>1</countRange>
                    </li>
                    <li MayRequire="Rick.FCP.EnergyWeapons">
                        <thingDef>FCP_Gun_Tri_Beam_Laser_Rifle</thingDef>
                        <countRange>1</countRange>
                    </li>
                </weightedDrops>
            </li>
            <li Class="CompProperties_UseEffectDestroySelf"/>
            <li Class="FCP.Core.CompProperties_LabelColored">
                <rarity>Rare</rarity>
            </li>
        </comps>
    </ThingDef>

