    <!-- BoS Appalachia T60 Power Armor -->

    <ThingDef ParentName="FCP_Apparel_Power_Armor_Base" MayRequire="Rick.FCP.PowerArmor">
        <defName>FCP_Apparel_BoS_Appalachia_T60_Power_Armor</defName>
        <label>BoS Appalachia T60 Power Armor</label>
        <description>Advanced T-60 power armor bearing the distinctive colors and insignia of the Appalachian Brotherhood of Steel. This battle-tested armor provides exceptional protection while maintaining mobility, representing the pinnacle of pre-war American military engineering adapted for wasteland operations.</description>
        <smeltable>false</smeltable>
        <thingClass>Apparel</thingClass>
        <thingSetMakerTags Inherit="False" />
        <possessionCount>1</possessionCount>
        <graphicData>
            <texPath>Things/Apparel/BoS_Power_Armor/BoS_Appalaccia_T60_Power_Armor/FCP_BoS_Appalaccia_T60_Power_Armor_Hulk_south</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>1.3</drawSize>
        </graphicData>
        <uiIconScale>1.3</uiIconScale>
        <tickerType>Normal</tickerType>
        <statBases>
            <MaxHitPoints>280</MaxHitPoints>
            <Mass>6.5</Mass>
            <MarketValue>3700</MarketValue>
            <ArmorRating_Sharp>3.00</ArmorRating_Sharp>
            <ArmorRating_Blunt>2.80</ArmorRating_Blunt>
            <ArmorRating_Heat>2.20</ArmorRating_Heat>
            <Insulation_Cold>6</Insulation_Cold>
            <Insulation_Heat>6</Insulation_Heat>
            <EquipDelay>1.1</EquipDelay>
        </statBases>
        <equippedStatOffsets>
            <MoveSpeed>0.13</MoveSpeed>
            <AimingDelayFactor>-0.07</AimingDelayFactor>
            <ToxicEnvironmentResistance>1.0</ToxicEnvironmentResistance>
            <SocialImpact>0.15</SocialImpact>
        </equippedStatOffsets>
        <apparel>
            <shellRenderedBehindHead>true</shellRenderedBehindHead>
            <bodyPartGroups>
                <li>Hands</li>
                <li>Feet</li>
                <li>Neck</li>
                <li>Legs</li>
                <li>Arms</li>
                <li>Torso</li>
                <li>Waist</li>
                <li>Shoulders</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Apparel/BoS_Power_Armor/BoS_Appalaccia_T60_Power_Armor/FCP_BoS_Appalaccia_T60_Power_Armor</wornGraphicPath>
            <layers>
